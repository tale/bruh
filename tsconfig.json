{
	"exclude": ["tsup.config.ts"],
	"compilerOptions": {
		"lib": ["ESNext"],
		"module": "ESNext",
		"target": "ESNext",
		"moduleResolution": "bundler",
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"baseUrl": "./src",
		"rootDir": "./src",
		"outDir": "./dist",
	}
}
